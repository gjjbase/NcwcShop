<?xml version="1.0" encoding="utf-8"?>
<com.ncwc.shop.widget.DamRebScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/topsroll"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.ncwc.shop.widget.AllListView
            android:id="@+id/listview_freerun"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:descendantFocusability="afterDescendants"
            android:divider="@null"
            android:focusable="false" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:text="@string/oldfree"
            android:textSize="20sp"
            android:textStyle="bold" />

        <com.ncwc.shop.widget.AllListView
            android:id="@+id/listview_freeold"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:descendantFocusability="afterDescendants"
            android:divider="@null" />
    </LinearLayout>
</com.ncwc.shop.widget.DamRebScrollView><!-- From: file:/Users/gaojiangjian/AndroidStudioProjects/NcwcShop/app/src/main/res/layout/fragment_freerun.xml -->