<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/BackColor">

    <!--标题-->
    <include
        layout="@layout/common_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"/>

    <!--店名-->
    <TextView
        android:id="@+id/tv_shouhou_shenqing_storename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/common_toolbar"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:text="你车我车"
        android:textColor="@color/Black"
        android:textSize="16sp"/>

    <!--下划线-->
    <ImageView
        android:id="@+id/img_17"
        android:layout_width="match_parent"
        android:layout_height="0.3dp"
        android:layout_below="@+id/tv_shouhou_shenqing_storename"
        android:layout_marginTop="4dp"
        android:src="@color/Gray"/>

    <!--商品列表-->
    <LinearLayout
        android:id="@+id/ll_shouhou_shenqing_pros"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_17"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="4dp"
        android:orientation="vertical">
        <!--代码动态生成产品信息-->
    </LinearLayout>

    <!--我要退货-->
    <!--<TextView
        android:id="@+id/tv_shouhou_shenqing_tuihuo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/ll_shouhou_shenqing_pros"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/shouhou_tui"
        android:textColor="@color/Gray"
        android:textSize="14sp"/>-->

</RelativeLayout>